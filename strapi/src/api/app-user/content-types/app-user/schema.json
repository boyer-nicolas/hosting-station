{
  "kind": "collectionType",
  "collectionName": "app_users",
  "info": {
    "singularName": "app-user",
    "pluralName": "app-users",
    "displayName": "App User",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "active": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "email": {
      "type": "email"
    },
    "password": {
      "type": "string",
      "required": true,
      "regex": "^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[#?!@$ %^&*-]).{8,}$"
    }
  }
}
